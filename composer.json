{
    "name": "bigpoint/php-sdk",
    "type": "library",
    "description": "Bigpoint PHP SDK",
    "keywords": ["bigpoint", "platform", "sdk", "oauth2", "REST"],
    "license": "Apache2",
    "autoload": {
        "psr-0": { "Bigpoint": "" }
    },
    "authors": [
        {
            "name": "Bernd Hoffmann"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "3.7.*@stable",
        "satooshi/php-coveralls": "dev-master",
        "sebastianbergmann/phpcov": "1.1.0"
    }
}